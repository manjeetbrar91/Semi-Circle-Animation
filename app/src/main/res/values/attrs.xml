<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SemiCircleAnimation">
        <attr name="inner_circle_color" format="color" />
        <attr name="inner_circle_width" format="dimension" />
        <attr name="animated_circle_color" format="color" />
        <attr name="animated_circle_width" format="dimension" />
        <attr name="primary_circle_border_color" format="color" />
        <attr name="primary_circle_border_width" format="dimension" />
        <attr name="anim_duration" format="integer" />
        <attr name="anim_reverse" format="boolean" />
        <attr name="anim_repeat" format="boolean" />

        <attr name="position" format="enum">
            <enum name="top_left" value="1" />
            <enum name="top_right" value="2" />
            <enum name="bottom_left" value="3" />
            <enum name="bottom_right" value="4" />
        </attr>


    </declare-styleable>


</resources>